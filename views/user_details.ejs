<h2><%=user.username%></h2>
<h3><%=user.full_name%></h3>
<img src="<%=user.profile_photo%>" alt="">

<% if (Number(userIdLoggedIn) === user.user_id) { %>
    <section>
    <button> <a href="/users/<%=user.user_id%>/edit">edit profile</a> </button>
    </section>
<% } %>

<section>
    <h4>Latest Workouts</h4>
    <ul>
        <% for (workout of workouts) { %>
        <li><a href="/workouts/<%=workout.workout_id%>"><%= workout.name %>, <%= workout.to_char %></a></li>
        <% } %> 
        <li>See More</li>
    </ul>
</section>